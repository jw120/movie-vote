{
  "name": "movie-vote-client",
  "version": "1.0.0",
  "description": "Client for movie voting app",
  "repository": {
    "type": "git",
    "url": "http://github.com/jw120/movie-vote-client.git"
  },
  "main": "index.js",
  "scripts": {
    "build:prod": "webpack --config webpack-prod.config.js",
    "build:dev": "webpack-dev-server --config webpack-dev.config.js --hot",
    "compile": "tsc",
    "lint": "tslint -c tslint.json src/*.ts* src/reducers/*.ts src/components/*.ts* src/containers/*.ts* test/*.ts test/reducers/*.ts test/components/*.ts*",
    "pretest": "tsc",
    "test": "mocha test test/components test/reducers",
    "coverage": "istanbul cover node_modules/.bin/_mocha -- -u exports -R spec test/*_spec.js test/reducers/*_spec.js test/components/*_spec.js"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "chai": "^3.4.1",
    "deep-freeze": "0.0.1",
    "jsx-chai": "^2.0.0",
    "mocha": "^2.3.4",
    "react-addons-test-utils": "^0.14.3",
    "react-hot-loader": "^1.3.0",
    "redux-devtools": "^3.0.0",
    "redux-devtools-dock-monitor": "^1.0.1",
    "redux-devtools-log-monitor": "^1.0.1",
    "ts-loader": "^0.7.2",
    "tslint": "^3.2.1",
    "typescript": "^1.7.5",
    "webpack": "^1.12.9",
    "webpack-dev-server": "^1.14.0"
  },
  "dependencies": {
    "bootstrap": "^3.3.6",
    "debug": "^2.2.0",
    "react": "^0.14.3",
    "react-bootstrap": "^0.28.1",
    "react-dom": "^0.14.3",
    "react-redux": "^4.0.0",
    "redux": "^3.0.4",
    "socket.io-client": "^1.3.7"
  }
}
