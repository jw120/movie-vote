{
  "name": "movie-vote-client",
  "version": "1.0.0",
  "description": "Client for movie voting app",
  "repository": {
    "type": "git",
    "url": "http://github.com/jw120/movie-vote-client.git"
  },
  "main": "index.js",
  "config": {
    "babelInputDir": "src/",
    "babelOutputDir": "compiled/",
    "mochaTestDirs": "test/ test/reducers"
  },
  "scripts": {
    "build": "webpack",
    "build:dev": "webpack-dev-server",
    "compile": "babel $npm_package_config_babelInputDir --out-dir $npm_package_config_babelOutputDir",
    "compile:watch": "babel -w -q $npm_package_config_babelInputDir --out-dir $npm_package_config_babelOutputDir",
    "lint": "eslint $npm_package_config_babelInputDir $npm_package_config_mochaTestDirs",
    "test": "mocha --compilers js:babel-core/register $npm_package_config_mochaTestDirs"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.2.0",
    "babel-core": "^6.2.1",
    "babel-eslint": "^5.0.0-beta4",
    "babel-loader": "^6.2.0",
    "babel-plugin-transform-flow-strip-types": "^6.1.18",
    "babel-plugin-transform-object-rest-spread": "^6.1.18",
    "babel-plugin-transform-strict-mode": "^6.2.0",
    "babel-plugin-undeclared-variables-check": "^6.1.18",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-react": "^6.1.18",
    "chai": "^3.4.1",
    "css-loader": "^0.23.0",
    "eslint": "^1.10.2",
    "eslint-config-standard": "^4.4.0",
    "eslint-plugin-react": "^3.11.1",
    "eslint-plugin-standard": "^1.3.1",
    "file-loader": "^0.8.5",
    "jsdom": "^7.1.0",
    "mocha": "^2.3.4",
    "react-addons-test-utils": "^0.14.3",
    "react-hot-loader": "^1.3.0",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.9",
    "webpack-dev-server": "^1.14.0"
  },
  "dependencies": {
    "bootstrap": "^3.3.6",
    "debug": "^2.2.0",
    "react": "^0.14.3",
    "react-bootstrap": "^0.28.1",
    "react-dom": "^0.14.3",
    "react-redux": "^4.0.0",
    "redux": "^3.0.4",
    "seamless-immutable": "^4.0.2",
    "socket.io-client": "^1.3.7"
  }
}
